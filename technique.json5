{
  name: "hello",
  uniforms: [
    { name: "u_transform", type: "mat4x4f" }
  ],
  shaders: [
    {
      type: "vertex",
      inputs: [
        { name: "a_position", type: "vec3f" }
      ],
      source: "
        void main() {
          rx_position = u_transform * vec4f(a_position, 1.0);
        }
      "
    },{
      type: "fragment",
      outputs: [
        { name: "fs_color", type: "vec4f" }
      ],
      source: "
        void main() {
          fs_color = vec4f(0.0, 1.0f, 0.0, 1.0f);
        }
      "
    }
  ]
}